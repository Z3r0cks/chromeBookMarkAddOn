{
   "chrome_url_overrides": {
      "newtab": "dist/index.html"
   },
   "name": "individualBookmarker",
   "version": "0.2",
   "manifest_version": 2,
   "description": "Test",
   "homepage_url": "https://example.org/my-addon",
   "content_scripts": [
      {
         "matches": [
            "http://localhost/"
         ],
         "js": [
            "dist/js/content.js"
         ]
      }
   ],
   "permissions": ["bookmarks", "storage" ],
   "browser_action": {
      "default_popup": "dist/index.html"
   }
}